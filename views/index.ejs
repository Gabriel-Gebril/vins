    <table class="table responsive-table table-striped table-hover">
        <thead>
            <tr>
                <th>Item</th>
                <th>In Stock</th>
                <th>Total</th>
            </tr>
        </thead>
        <tbody>
            <%if(isNaN(page)){%>
                <%page = 0;%>
            <%}%>
            <%for(var i = 0; i<item.length; i++){%>
            <tr>
                <td><%= item[i].name %></td>
                <td><%= item[i].instock %></td>
                <td><%= item[i].total %></td>
            </tr>

            <%}%>   
        </tbody>
    </table>



